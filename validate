<!DOCTYPE html>
<html>
    <head>
        <title></title>
    </head>
    <body>
        <input type="number" name="username"  id="btn" required>
        <button onclick="validate()">Click Here</button>
        <p id="demo"></p>
        <script>
        function validate(){
             var obj=document.getElementById("btn");
             if(!obj.checkValidity()){
                 document.getElementById("demo").innerHTML=obj.validationMessage;
             }
             else{
                 document.getElementById("demo").innerHTML="Input ok";
             }
        }
    </script>
    </body>
    
</html>
